{
  "site": {
    "title": "GuloKlopo",
    "base_url": "https://astroplate.netlify.app",
    "base_path": "/",
    "trailing_slash": false,
    "favicon": "/images/icon-gulo.png",
    "logo": "/images/GuloKlopo-green.png",
    "logo_darkmode": "/images/GuloKlopo-darkmode.png",
    "logo_width": "150",
    "logo_height": "30",
    "logo_text": "GuloKlopo"
  },

  "settings": {
    "search": true,
    "sticky_header": true,
    "theme_switcher": true,
    "default_theme": "system",
    "pagination": 2,
    "summary_length": 200,
    "blog_folder": "blog"
  },

  "params": {
    "contact_form_action": "#",
    "copyright": ""
  },

  "navigation_button": {
    "enable": true,
    "label": "Hubungi Kami",
    "link": "/contact"
  },

  "disqus": {
    "enable": true,
    "shortname": "themefisher-template",
    "settings": {}
  },

  "metadata": {
    "meta_author": "madeitastro",
    "meta_image": "/images/og-image.png",
    "meta_description": "madewithastro"
  }
}
